{
  "store.sellers": {
    "around": ["productWrapper"],
    "context": "vtex.store/ProductContext"
  },
  "store.product.sellertest": {
    "allowed": ["link-seller"]
  },
  "link-seller": {
    "component": "LinkSeller"
  },
  "seller-table": {
    "component": "SellerSelectorTable",
    "allowed": ["seller-head"],
    "composition": "children"
  },
  "seller-head": {
    "component": "SellerSelectorHead",
    "composition": "children"
  },
  "seller-head-cell": {
    "component": "SellerSelectorHeadCell"
  }
}
